{
  "name": "test-architeture",
  "version": "0.0.1",
  "description": "first test architeture version",
  "main": "index.js",
  "scripts": {
    "test:run": "cypress-tags run -e TAGS=@testRun --browser chrome --headed --no-exit",
    "report": "node cypress/plugins/index.js",
    "pretest": "rm -rf cypress/test-results/cucumber-json cypress/test-results/html cypress/videos",
    "postcypress:run": "node cypress/report/report.js"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/support/steps",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/test-results/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "dependencies": {
    "cucumber-html-reporter": "^5.5.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-tags": "^0.3.0",
    "multiple-cucumber-html-reporter": "^1.18.3"
  },
  "devDependencies": {
    "cypress": "^15.9.0",
    "cypress-dotenv": "^2.0.0",
    "dotenv": "^10.0.0"
  }
}
